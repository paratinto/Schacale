PUBLIC SUB GradeBox_Click()
  PRINT gradeBox.Text
  IF GradeBox.Text = "--choose--" THEN PictureBox1.Picture = Picture.Load("./pic/user/unknown.png")
  IF GradeBox.text = "Student" THEN PictureBox1.Picture = Picture.Load("./pic/user/student.png")
  IF GradeBox.text = "Bachelor" THEN PictureBox1.Picture = Picture.Load("./pic/user/student.png")
  IF GradeBox.text = "Master / Diploma" THEN PictureBox1.Picture = Picture.Load("./pic/user/diploma.png")
  IF GradeBox.text = "Postdoc" THEN PictureBox1.Picture = Picture.Load("./pic/user/doc.png")
  IF GradeBox.text = "Professor" THEN PictureBox1.Picture = Picture.Load("./pic/user/doc.png")
  IF GradeBox.text = "Assistant" THEN PictureBox1.Picture = Picture.Load("./pic/user/student.png")
END

